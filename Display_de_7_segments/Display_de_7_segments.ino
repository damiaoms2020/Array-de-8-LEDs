int A=5;
int B=6;
int C=7;
int D=8;
int E=9;
int F=10;
int G=11;

void setup() {
  
    pinMode(A, OUTPUT);
    pinMode(B, OUTPUT);
    pinMode(C, OUTPUT);
    pinMode(D, OUTPUT);
    pinMode(E, OUTPUT);
    pinMode(F, OUTPUT);
    pinMode(G, OUTPUT);
    

}

void loop() {
  digitalWrite(A,HIGH);
  delay(1000);
  digitalWrite(B,HIGH);
  delay(1000);
  digitalWrite(C,HIGH);
  delay(1000);
  digitalWrite(D,HIGH);
  delay(1000);
  digitalWrite(E,HIGH);
  delay(1000);
  digitalWrite(F,HIGH);
  delay(1000);
  digitalWrite(G,HIGH);
  delay(1000);

}
